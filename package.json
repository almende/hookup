{
  "name": "hookup",
  "version": "0.0.1",
  "description": "Hook up with peers by id using a WebRTC connection",
  "main": "demo/server.js",
  "scripts": {
    "start": "npm run dev-server",
    "test": "mocha",
    "build": "npm run build-peer; npm run build-broker",
    "build-peer": "rollup --config rollup-peer.config.js",
    "build-broker": "rollup --config rollup-broker.config.js",
    "watch": "watch 'npm run build' ./src",
    "dev-server": "DEBUG=hookup:* babel-node src/broker/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enmasseio/hookup.git"
  },
  "keywords": [
    "webrtc",
    "connect",
    "peer",
    "p2p"
  ],
  "author": "Jos de Jong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enmasseio/hookup/issues"
  },
  "homepage": "https://github.com/enmasseio/hookup#readme",
  "dependencies": {
    "babel-cli": "6.5.1",
    "babel-preset-es2015-rollup": "1.1.1",
    "debug": "2.2.0",
    "emitter-component": "1.1.1",
    "express": "4.13.4",
    "node-uuid": "1.4.7",
    "ws": "1.0.1"
  },
  "devDependencies": {
    "mocha": "2.4.5",
    "reconnectingwebsocket": "1.0.0",
    "rollup": "0.25.3",
    "rollup-plugin-babel": "2.3.9",
    "rollup-plugin-commonjs": "2.2.1",
    "rollup-plugin-node-resolve": "1.4.0",
    "rollup-plugin-uglify": "0.2.0",
    "simple-peer": "5.12.0",
    "watch": "0.17.1"
  }
}
