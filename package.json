{
  "name": "linkup",
  "version": "0.0.1",
  "description": "Link up peers by id using a WebRTC connection",
  "main": "dist/linkup.js",
  "scripts": {
    "start": "npm run build; DEBUG=linkup:* node dist/broker/server.js",
    "test": "mocha --compilers js:babel-register",
    "build": "npm run build-peer; npm run build-broker; npm run copy-demo; npm run replace-demo",
    "build-peer": "rollup --config rollup-peer.config.js",
    "build-broker": "rollup --config rollup-broker.config.js",
    "copy-demo": "cp demo/browser_peer.html dist/index.html; ",
    "replace-demo": "replace '<script src=\"https://linkup-broker.herokuapp.com/linkup.js\"></script>' '<script src=\"linkup.js\"></script>' dist/index.html",
    "watch": "watch 'npm run build' ./src",
    "dev-server": "DEBUG=linkup:* babel-node src/broker/server.js",
    "deploy": "git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enmasseio/linkup.git"
  },
  "keywords": [
    "webrtc",
    "connect",
    "peer",
    "p2p"
  ],
  "author": "Jos de Jong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enmasseio/linkup/issues"
  },
  "homepage": "https://github.com/enmasseio/linkup#readme",
  "dependencies": {
    "babel-preset-es2015-rollup": "1.1.1",
    "debug": "2.2.0",
    "emitter-component": "1.1.1",
    "express": "4.13.4",
    "node-uuid": "1.4.7",
    "reconnectingwebsocket": "1.0.0",
    "replace": "0.3.0",
    "rollup": "0.25.4",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-commonjs": "2.2.1",
    "rollup-plugin-node-resolve": "1.4.0",
    "rollup-plugin-uglify": "0.2.0",
    "simple-peer": "6.0.2",
    "ws": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.0",
    "babel-register": "6.6.0",
    "mocha": "2.4.5",
    "watch": "0.17.1"
  }
}
